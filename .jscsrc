{
  "excludeFiles": [
    "node_modules/**",
    "bower_components/**",
    "example/bower_components/**"
  ],
  "disallowSpacesInsideArrayBrackets": {
    "allExcept": [
      "[",
      "]",
      "{",
      "}"
    ]
  },
  "safeContextKeyword": ["self"],
  "disallowSpacesInAnonymousFunctionExpression": {
    "beforeOpeningRoundBrace": true
  },
  "maximumLineLength": 120,
  "validateQuoteMarks": "'"
}